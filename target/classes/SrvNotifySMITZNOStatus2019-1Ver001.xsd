		<xs:restriction base="C">
			<xs:maxLength value="40"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="C4000_Type">
		<xs:annotation>
			<xs:documentation>Символьный тип с ограниченим длины в 4000 символов</xs:documentation>
		</xs:annotation>
		<xs:restriction base="C">
			<xs:maxLength value="4000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="C50_Type">
		<xs:annotation>
			<xs:documentation>Символьный тип с ограниченим длины в 50 символов</xs:documentation>
		</xs:annotation>
		<xs:restriction base="C">
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="C64_Type">
		<xs:annotation>
			<xs:documentation>Тип: Символьный тип, max длиной 64 символов</xs:documentation>
		</xs:annotation>
		<xs:restriction base="C">
			<xs:maxLength value="64"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="C70_Type">
		<xs:annotation>
			<xs:documentation>Тип: Символьный тип, max длиной 70 символов</xs:documentation>
		</xs:annotation>
		<xs:restriction base="C">
			<xs:maxLength value="70"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="C80">
		<xs:restriction base="C">
			<xs:maxLength value="80"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DateTime">
		<xs:annotation>
			<xs:documentation>Дата и время</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="([0-9]{4}-[0-9]{2}-[0-9]{2}(T[0-9]{2}:[0-9]{2}(:[0-9]{2}(\.[0-9]{1,6})?((-|\+)[0-9]{2}:[0-9]{2}|Z?)?)?)?)|(\d{14}) "/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="EsoTemplateId_Type">
		<xs:annotation>
			<xs:documentation>Тип:Код шаблона обращения ЕСО</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Long"/>
	</xs:simpleType>
	<xs:simpleType name="Long">
		<xs:annotation>
			<xs:documentation>Длинное число.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:long"/>
	</xs:simpleType>
	<xs:simpleType name="NC">
		<xs:annotation>
			<xs:documentation>Строковый тип в кодировке ISO Latin-1.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="OperUID_Type">
		<xs:annotation>
			<xs:documentation>Уникальный идентификатор операции</xs:documentation>
		</xs:annotation>
		<xs:restriction base="C">
			<xs:minLength value="1"/>
			<xs:maxLength value="32"/>
			<xs:pattern value="[0-9a-zA-Z]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RbBrchType">
		<xs:annotation>
			<xs:documentation>Тип данных для 6-значного кода территориального банка. Используется для передачи кода ТБ, в котором обслуживается продукт клиента</xs:documentation>
		</xs:annotation>
		<xs:restriction base="C">
			<xs:maxLength value="6"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RbTbBrchType">
		<xs:annotation>
			<xs:documentation>Тип данных для 8-значного кода территориального банка. Используется для передачи кода ТБ, в котором открыт счет МБК, по которой клиент произвел идентификацию на УС</xs:documentation>
		</xs:annotation>
		<xs:restriction base="C">
			<xs:maxLength value="8"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RegionId_Type">
		<xs:annotation>
			<xs:documentation>Код региона банка. Используется для обозначения кода ДБ</xs:documentation>
		</xs:annotation>
		<xs:restriction base="NC">
			<xs:maxLength value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RqUID_Type">
		<xs:annotation>
			<xs:documentation>
				Идентификатор запроса.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="UUID_Type">
			<xs:pattern value="[0-9a-zA-Z]{32}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ServerStatusCode_Type">
		<xs:annotation>
			<xs:documentation>
				Статусный код сервера. Это значение выдается пользователю на экран и позволяет провайдеру лучше диагностировать ошибку.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="C">
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ServerStatusType_Type">
		<xs:annotation>
			<xs:documentation>Исходное описание ошибки из внешней системы (заполняется только при значении StatusCode, отличном от 0)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="C">
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SPName_Type">
		<xs:annotation>
			<xs:documentation>Идентификатор системы</xs:documentation>
		</xs:annotation>
		<xs:restriction base="C">
			<xs:maxLength value="255"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StatusCode_Type">
		<xs:annotation>
			<xs:documentation>
				Статусный код возврата.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Long"/>
	</xs:simpleType>
	<xs:simpleType name="StatusDesc_Type">
		<xs:annotation>
			<xs:documentation>
				Описание статуса.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="C">
			<xs:maxLength value="1000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StatusType_Type">
		<xs:annotation>
			<xs:documentation>Описание ответа - тип ошибки (заполняется только при значении StatusCode, отличном от 0)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="C">
			<xs:maxLength value="22"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TaxId_Type">
		<xs:annotation>
			<xs:documentation>Идентификатор налогоплательщика (ИНН).
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="NC">
			<xs:maxLength value="32"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="UUID_Type">
		<xs:annotation>
			<xs:documentation>Уникальный 16 байтовый идентификатор. Для обмена в рамках интеграционных проектов Сбербанка представляется набором из 32 символов
				без разделителей в шестнадцатиричной системе./
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="NC">
			<xs:pattern value="[0-9a-zA-Z]{32}"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
